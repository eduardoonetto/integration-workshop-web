<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
</IfModule>

# Excepciones para archivos CSS, JavaScript e im√°genes
RewriteRule ^css/ - [L]
RewriteRule ^js/ - [L]
RewriteRule ^images/ - [L]
